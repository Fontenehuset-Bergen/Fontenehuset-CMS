{
  "name": "fontenehuset-cms",
  "private": true,
  "version": "1.1.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "validate": "sanity schema validate --workspace production-workspace",
    "typegen": "npm run validate && sanity schema extract --workspace production-workspace --path export/schema.json && sanity typegen generate",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/icons": "^3.7.0",
    "@sanity/ui": "^3.0.14",
    "@sanity/vision": "^4.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^4.6.1",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^18.0.25",
    "eslint": "^9.9.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "always",
    "jsxSingleQuote": true,
    "singleAttributePerLine": false,
    "bracketSameLine": false,
    "bracketSpacing": true
  }
}
